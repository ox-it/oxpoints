    <xs:complexType>
      <xs:sequence>
        <xs:choice>
          <xs:group minOccurs="0" maxOccurs="unbounded" ref="ns1:model.pLike"/>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:group ref="ns1:model.labelLike"/>
            <xs:group ref="ns1:model.placeStateLike"/>
